{
  "name": "token-speed-tester",
  "version": "1.6.0",
  "description": "A CLI tool to test LLM API token output speed",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "token-speed-test": "dist/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "README.en.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup && tsc --noEmit",
    "dev": "tsx src/index.ts",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest --run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --run --coverage",
    "test:watch": "vitest",
    "prepublishOnly": "npm run build",
    "prepare": "husky"
  },
  "keywords": [
    "llm",
    "token",
    "speed",
    "test",
    "anthropic",
    "openai",
    "cli"
  ],
  "author": "Cansiny0320",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Cansiny0320/token-speed-tester.git"
  },
  "bugs": {
    "url": "https://github.com/Cansiny0320/token-speed-tester/issues"
  },
  "homepage": "https://github.com/Cansiny0320/token-speed-tester#readme",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.32.1",
    "chalk": "^5.4.1",
    "cli-table3": "^0.6.5",
    "commander": "^12.1.0",
    "open": "^11.0.0",
    "openai": "^4.77.3",
    "string-width": "^7.2.0",
    "tiktoken": "^1.0.22"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^13.1.3",
    "@types/node": "^22.10.6",
    "@types/open": "^6.1.0",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "parser": "^0.1.4",
    "prettier": "^3.7.4",
    "semantic-release": "^25.0.2",
    "tsup": "^8.5.1",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.52.0",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
