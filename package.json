{
  "name": "token-speed-tester",
  "version": "1.11.2",
  "description": "A CLI tool to test LLM API token output speed",
  "type": "module",
  "packageManager": "pnpm@10.25.0",
  "author": "Cansiny0320",
  "license": "MIT",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Cansiny0320/token-speed-tester.git"
  },
  "bugs": {
    "url": "https://github.com/Cansiny0320/token-speed-tester/issues"
  },
  "homepage": "https://github.com/Cansiny0320/token-speed-tester#readme",
  "keywords": [
    "llm",
    "token",
    "speed",
    "test",
    "anthropic",
    "openai",
    "cli"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "bin": {
    "token-speed-tester": "dist/index.mjs"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist",
    "README.md",
    "README.en.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsdown && node scripts/copy-assets.mjs",
    "dev": "tsx src/index.ts",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "prepublishOnly": "nr build",
    "release": "bumpp",
    "start": "node dist/index.mjs",
    "test": "vitest --run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --run --coverage",
    "test:watch": "vitest",
    "typecheck": "tsc",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.32.1",
    "chalk": "^5.4.1",
    "commander": "^12.1.0",
    "open": "^11.0.0",
    "openai": "^4.77.3",
    "string-width": "^7.2.0",
    "tiktoken": "^1.0.22"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.6.1",
    "@antfu/ni": "^28.0.0",
    "@types/node": "^22.10.6",
    "@vitest/coverage-v8": "^2.1.9",
    "@vitest/ui": "^2.1.9",
    "bumpp": "^10.3.2",
    "eslint": "^9.39.2",
    "lint-staged": "^16.2.7",
    "simple-git-hooks": "^2.13.1",
    "tsdown": "^0.17.3",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vitest": "^2.1.9"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm i --frozen-lockfile --ignore-scripts --offline && npx lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
